{
    "extends": "airbnb-base",
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "plugins": [
        "import"
    ],
    "rules": {
        "brace-style": "off",
        "indent": ["error", 4, { "SwitchCase": 1 }]
    }
}
